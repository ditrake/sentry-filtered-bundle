{
    "type": "symfony-bundle",
    "name": "srr/sentry-filtered-bundle",
    "description": "Sentry filtered bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Razin",
            "email": "srr@webcrt.ru"
        }
    ],
    "require": {
        "php": "^7.4",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/yaml": "4.4.*",
        "sensio/framework-extra-bundle": "^5.4",
        "sentry/sentry-symfony": "^3.2"
    },
    "require-dev": {
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "srr\\SentryFilteredBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "srr\\SentryFilteredBundle\\Tests\\": "test"
        }
    }
}
