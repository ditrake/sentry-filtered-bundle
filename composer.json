{
    "type": "symfony-bundle",
    "name": "srr/sentry-filtered-bundle",
    "description": "Sentry filtered bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Razin",
            "email": "srr@webcrt.ru"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/yaml": "4.4.*",
        "sensio/framework-extra-bundle": "^5.4",
        "sentry/sentry-symfony": "^3.2"
    },
    "require-dev": {
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.1",
        "codeception/module-cli": "^1.0",
        "codeception/module-doctrine2": "^1.0",
        "codeception/module-symfony": "^1.0",
        "vimeo/psalm": "^3.8"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "srr\\SentryFilteredBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "srr\\SentryFilteredBundle\\Tests\\": "test"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
